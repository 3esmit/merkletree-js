{
  "name": "@jharrilim/merkletree-js",
  "version": "1.1.0",
  "description": "JavaScript implementation of a Merkle Tree.",
  "scripts": {
    "build": "tsc",
    "coverage": "nyc npm test",
    "report": "nyc report --reporter=text-lcov",
    "test": "mocha",
    "typedoc": "typedoc --out docs src"
  },
  "author": "Joseph Harrison-Lim",
  "homepage": "https://github.com/jharrilim/merkletree-js#readme",
  "license": "GPL-3.0",
  "keywords": [
    "merkletree",
    "merkle",
    "tree",
    "merkle-tree"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jharrilim/merkletree-js.git"
  },
  "bugs": {
    "url": "https://github.com/jharrilim/merkletree-js/issues"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/chai-as-promised": "^7.1.0",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.13.9",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "mocha": "^6.1.4",
    "nyc": "^14.1.0",
    "source-map-support": "^0.5.12",
    "ts-node": "^8.1.0",
    "typedoc": "0.14.2",
    "typescript": "^3.4.5"
  },
  "files": [
    "index.js",
    "index.js.map",
    "index.d.ts"
  ],
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "coverage",
      "docs",
      "dist",
      "test",
      "**/*.d.ts"
    ],
    "reporter": [
      "html",
      "json",
      "text",
      "lcov"
    ],
    "all": true
  },
  "publishConfig": {
    "access": "public"
  }
}
